{
  "name": "29k-cms",
  "version": "2.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build:content": "yarn --cwd \"../content\" build",
    "build": "yarn build:content && rm -rf dist && vite build",
    "build:types": "yarn ts-node ./generateTypes.ts",
    "dev": "yarn build:content && vite & PORT=1337 GIT_REPO_DIRECTORY='./../' yarn run netlify-cms-proxy-server",
    "start": "yarn build:content && vite",
    "test:jest": "jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "test:ts": "tsc --noEmit",
    "test": "yarn build:content && yarn test:ts && yarn test:lint && yarn test:jest"
  },
  "dependencies": {
    "i18next": "^23.7.6",
    "netlify-cms-app": "^2.15.72",
    "netlify-cms-media-library-cloudinary": "^1.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "netlify-cms-proxy-server": "^1.3.24",
    "netlify-ts": "^2.0.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
