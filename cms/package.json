{
  "name": "29k-cms",
  "version": "2.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build:content": "yarn --cwd \"../content\" build",
    "build": "yarn build:content && rm -rf dist && vite build",
    "build:types": "yarn ts-node ./generateTypes.ts",
    "dev": "yarn build:content && vite & PORT=1337 GIT_REPO_DIRECTORY='./../' yarn run decap-server",
    "start": "yarn build:content && vite",
    "test:jest": "jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "test:ts": "tsc --noEmit",
    "test": "yarn build:content && yarn test:ts && yarn test:lint && yarn test:jest"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "decap-cms-app": "3.1.0-beta.3",
    "decap-cms-media-library-cloudinary": "^3.0.1",
    "i18next": "^23.8.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-web": "^0.19.10",
    "styled-components": "^6.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-native-web": "npm:@types/react-native",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "decap-server": "^3.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "netlify-ts": "^2.0.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
