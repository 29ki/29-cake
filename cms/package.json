{
  "name": "29k-cms",
  "version": "2.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build:content": "yarn --cwd \"../content\" build",
    "build": "yarn build:content && rm -rf dist && vite build",
    "build:types": "yarn ts-node ./generateTypes.ts",
    "dev": "yarn build:content && vite & PORT=1337 GIT_REPO_DIRECTORY='./../' yarn run netlify-cms-proxy-server",
    "start": "yarn build:content && vite",
    "test:jest": "jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "test:ts": "tsc --noEmit",
    "test": "yarn build:content && yarn test:ts && yarn test:lint && yarn test:jest"
  },
  "dependencies": {
    "i18next": "^22.4.10",
    "netlify-cms-app": "^2.15.72",
    "netlify-cms-media-library-cloudinary": "^1.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "netlify-cms-proxy-server": "^1.3.24",
    "netlify-ts": "^2.0.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.3"
  }
}
