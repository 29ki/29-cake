{
  "name": "29k-cms",
  "version": "2.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build:content": "yarn --cwd \"../content\" build",
    "build": "yarn build:content && rm -rf dist && vite build",
    "build:types": "yarn ts-node ./generateTypes.ts",
    "dev": "yarn build:content && vite & PORT=1337 GIT_REPO_DIRECTORY='./../' yarn run netlify-cms-proxy-server",
    "start": "yarn build:content && vite",
    "test:jest": "jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "test:ts": "tsc --noEmit",
    "test": "yarn build:content && yarn test:ts && yarn test:lint && yarn test:jest"
  },
  "dependencies": {
    "i18next": "^23.2.8",
    "netlify-cms-app": "^2.15.72",
    "netlify-cms-media-library-cloudinary": "^1.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "netlify-cms-proxy-server": "^1.3.24",
    "netlify-ts": "^2.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
